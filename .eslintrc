{
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ]
    },
    "plugins": [
      "prettier"
    ],
    
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "[javascript]": {
    "editor.formatOnSave": true,
    },
    "[html]": {
    "editor.formatOnSave": false,
    },
    "[json]": {
    "editor.formatOnSave": false,
    },
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,

  }